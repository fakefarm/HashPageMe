<h2>Contact @<%= @username %></h2>
<p class='fa fa-twitter'>
  <%= link_to @username, "http://www.twitter.com/#{@username}" %>
</p>
<p>
  <% @contact_tweets.each do |t| %>
    <span>#email </span>
    <%= link_to tweet_path(t), target: '_blank' do %>
      <%= only_text(t.text) %>
      <% if  t.image.present? %>
        <i class="fa fa-camera-retro"></i>
      <% end %>
    <% end %>
    &nbsp;
  <% end %>
</p>
