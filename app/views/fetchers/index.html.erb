<h1>Hashpages</h1>

<div class="l-container">
  <table>
    <thead>
      <tr>
        <th colspan="3"></th>
      </tr>
    </thead>

    <tbody>
      <% @fetchers.each do |fetcher| %>
        <tr>
          <td><%= link_to "#{fetcher.username}", "/#{fetcher.username}" %></td>
        </tr>
      <% end %>
    </tbody>
  </table>
</div>
