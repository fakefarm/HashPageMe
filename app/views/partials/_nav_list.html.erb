<nav>
  <ul>
    <li><%= link_to 'home', "/#{@username}" %></li>
    <li><%= link_to 'about', "/#{@username}/about" %></li>
  </ul>
  <div><hr></div>
  <ul>
    <% @hashtags.each do |tag| %>
      <li><%= link_to tag, "/#{@username}/#{tag}" %></li>
    <% end %>
  </ul>
  <div><hr></div>
  <ul>
    <li><%= link_to "http://www.twitter.com/#{@username}" do %>
        <i class='fa fa-twitter'></i>
      <% end %>
    </li>
  </ul>
</nav>
